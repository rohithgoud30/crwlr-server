# API Settings
ENVIRONMENT=development
API_KEY=your_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud SQL Settings
DB_USER=postgres
DB_PASS=your_secure_password
DB_NAME=crwlr
INSTANCE_CONNECTION_NAME=your-project-id:your-region:your-instance-name

# Optional: For local development with direct connection
# DB_HOST=127.0.0.1
# DB_PORT=5432

# Optional: For Cloud SQL Auth Proxy with Unix Socket
# INSTANCE_UNIX_SOCKET=/cloudsql/your-project-id:your-region:your-instance-name

# Optional: For private IP connection
# PRIVATE_IP=TRUE

# CORS Settings (comma-separated list)
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8000

AUTO_START_PROXY=true